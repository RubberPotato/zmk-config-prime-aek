#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
// ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮          ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮
// |  ESC     |  Q       |  W       |  E       |  R       |  T       |          |  Y       |  U       |  I       |  O       |  P       |  [       | Del      |
     &kp ESC    &kp Q      &kp W      &kp E      &kp R      &kp T                 &kp Y      &kp U      &kp I      &kp O      &kp P      &kp LBKT   &kp DEL
// ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┤          ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┴──────────┤
// |  FN1     |  A       |  S       |  D       |  F       |  G       |          |  H       |  J       |  K       |  L       |  ;       |  Enter              |
     &mo 1     &kp A      &kp S      &kp D      &kp F      &kp G                 &kp H      &kp J      &kp K      &kp L      &kp SEMI   &kp ENTER
// ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┤          ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┬──────────┤
// |  L SHIFT |  Z       |  X       |  C       |  V       |  B       |          |  b       |  N       |  M       |  ,       |  .       |  /       |  BT      |
     &kp LSHFT  &kp Z      &kp X      &kp C      &kp V      &kp B                 &kp B      &kp N      &kp M      &kp COMMA  &kp DOT    &kp SLASH  &mo 1
// ├──────────┼──────────┴──────────┼──────────┼──────────┴──────────┤          ├──────────┴──────────┼──────────┼──────────┴──────────┤──────────┤──────────┤
// |  CMD     |                     |  LALT    |  SPACE              |          |  FN                 |  ALT     |                     |  CMD     |  R CTRL  |
     &kp LGUI                         &kp LALT   &lt 3 SPACE                      &mo 2                 &kp RCTRL                         &mo 4     &kp RCTRL
// ╰──────────╯                     ╰──────────┴─────────────────────╯          ╰─────────────────────┴──────────╯                     ╰──────────┴──────────╯
            >;
        };
        Func_layer {
            bindings = <
// ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮          ╭──────────┬──────────┬──────────┬──────────┬──────────┬──────────┬──────────╮
// |  x       |  HOME    |  UP      |  END     |  x       |  x       |          |  F1      |  F2      |  F3      |  F4      |  x       |  x       | x        |
     &trans     &kp HOME   &kp UP     &kp END     &trans     &trans                &kp F1     &kp F2    &kp F3    &kp F4     &trans     &trans    &trans
// ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┤          ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┴──────────┤
// |  x       |  LEFT    |  DOWN    |  RIGHT   |  x       |  x       |          |  F5      |  F6      |  F7      |  F8      |  x       |  x                  |
     &trans     &kp LEFT   &kp DOWN   &kp RIGHT  &trans     &trans               &kp F5      &kp F6     &kp F7     &kp F8     &trans     &trans       
// ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┤          ├──────────┼──────────┼──────────┼──────────┼──────────┤──────────┬──────────┤
